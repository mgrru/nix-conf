global {
  lan_interface: docker0, vboxnet0, enp0s3, lo
  wan_interface: auto
  log_level: info
  allow_insecure: false
  auto_config_kernel_parameter: true

  tcp_check_url: 'http://cp.cloudflare.com'
  tcp_check_http_method: HEAD
}

subscription {
  sub1: 'https://66bc03.xfxcloud.click/api/v1/client/subscribe?token=5a81455c3edb4e421280b26b007f5c40'
}

dns {
  upstream {
    trdns: 'udp://180.184.1.1:53'
    googledns: 'tcp+udp://8.8.8.8:53'
    cfdns: 'tcp+udp://1.1.1.1:53'
  }

  routing {
#    request {
#      qname(geosite:cn) -> trdns
#      fallback: googledns
#    }

    request {
      fallback: trdns
    }
    response {
      upstream(googledns) -> accept
      ip(geoip:private) && !qname(geosite:cn) -> googledns
      fallback: accept
    }
  }
}

group {
  proxy {
    filter: subtag(sub1) && !name(keyword: 'ExpireAt:')
    policy: min_moving_avg
  }
}

routing {
  pname(NetworkManager) -> direct
  dip(224.0.0.0/3, 'ff00::/8') -> direct


  l4proto(udp) && dport(443) -> block
  dip(geoip:private) -> direct
  dip(geoip:cn) -> direct
  domain(geosite:cn) -> direct

  fallback: proxy
}
